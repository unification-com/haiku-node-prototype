********************** BINARY DATA BLOBS, STRUCTURED DATABASE *************
create database Datablobs;
use database Datablobs;

CREATE TABLE BlobCreator (
    CreatorID int NOT NULL,
	Name varchar(255),
    PRIMARY KEY (CreatorID)
);

CREATE TABLE BlobData (
    BlobID int NOT NULL,
    DataBlob BLOB,
	BlobSize int,
	CreatorID int,
    PRIMARY KEY (BlobID),
    FOREIGN KEY (CreatorID) REFERENCES BlobCreator(CreatorID)
);


**************** HEARTRATE DATABASE ***************************
create database heartbit;
use heartbit;

CREATE TABLE Users (
    ID int NOT NULL,
    LastName varchar(255) NOT NULL,
    FirstName varchar(255),
    Email varchar(255),
    PRIMARY KEY (ID)
);

CREATE TABLE UserData (
    DataID int NOT NULL,
    Heartrate int,
    Pulse int,
	UserID int NOT NULL,
    PRIMARY KEY (DataID),
    FOREIGN KEY (UserID) REFERENCES Users(ID)
);

